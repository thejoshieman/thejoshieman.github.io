{
  "name": "thejoshieman.github.io",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve",
    "start": "node server.js",
    "buildDev": "ng build --watch --configuration development",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "deploy": "gh-pages -d build",
    "test": "ng test --watch false",
    "coverage": "ng test --no-watch --code-coverage --browsers=ChromeHeadlessCI",
    "e2e": "nightwatch  -r ./e2e/html-reporter.js nightwatch.conf.js -e chrome",
    "e2e:test": "./node_modules/.bin/nightwatch --env 'chrome' --config './nightwatch.conf.js'",
    "e2e:firefox": "nightwatch -r e2e/html-reporter.js -c nightwatch.conf.js -e firefox",
    "e2e:safari": "nightwatch -r e2e/html-reporter.js -c nightwatch.conf.js -e safari",
    "e2e:edge": "nightwatch  -r e2e/html-reporter.js nightwatch.conf.js -e edge",
    "e2e:chrome": "nightwatch  -r e2e/html-reporter.js nightwatch.conf.js -e chrome",
    "dockerBuild": "docker build -f Dockerfile -t thejoshieman.com .",
    "dockerRun": "docker run  --env=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin --workdir=/ -p 8080:8080 --runtime=runc -d thejoshieman.com:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.x",
    "@angular/common": "^19.0.x",
    "@angular/compiler": "^19.0.x",
    "@angular/core": "^19.0.x",
    "@angular/platform-browser": "^19.0.x",
    "@angular/platform-browser-dynamic": "^19.0.x",
    "@angular/router": "^19.0.x",
    "bootstrap": "^5.3.x",
    "express": "^4.21.x",
    "express-rate-limit": "~7.5.x",
    "rxjs": "^7.8.x",
    "zone.js": "^0.15.x"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.x",
    "@angular/cli": "^19.0.x",
    "@angular/compiler-cli": "^19.0.x",
    "@angular/localize": "^19.0.x",
    "@cucumber/cucumber": "~11.1.0",
    "@cucumber/pretty-formatter": "~1.0.1",
    "@nightwatch/angular": "~3.1.1",
    "@nightwatch/mobile-helper": "~1.0.2",
    "@nightwatch/schematics": "~1.2.1",
    "@swc/core": "~1.10.6",
    "@types/chai": "~5.2.1",
    "@types/cucumber": "~7.0.0",
    "@types/jasmine": "~5.1.4",
    "@types/mocha": "~10.0.6",
    "@types/nightwatch": "3.0.1",
    "@types/node": "~22.10.5",
    "chai": "~5.2.0",
    "chromedriver": "~131.0.5",
    "cucumber": "~6.0.7",
    "cucumber-html-reporter": "~6.0.0",
    "cucumber-pretty": "~6.0.1",
    "fs": "~0.0.1-security",
    "geckodriver": "~5.0.0",
    "gh-pages": "~6.3.0",
    "handlebars": "~4.7.8",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "~2.0.1",
    "karma-spec-reporter": "~0.0.36",
    "mocha": "~11.0.1",
    "nightwatch": "~3.10.0",
    "nightwatch-html-reporter": "~2.0.6",
    "nohup": "~0.1.0",
    "path": "~0.12.7",
    "ts-node": "~10.9.2",
    "tslib": "~2.8.1",
    "typescript": "~5.6.3"
  },
  "publishConfig": {
    "registry": "https://github.com/thejoshieman/"
  }
}
